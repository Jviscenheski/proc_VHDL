	# chamei o acumulador de r1


	#	MOVA A,0		#zera o acumulador
	addi $s1,$zero,0		#zera o acumulador
	
	#	ADD A,30		# coloca 30 no acumulador	
	addi $s1,$s1,30			# joga 30 no acumulador
	
	addi $s6,$zero,0		#zera o R6
	
	#	MOV R6,A		#coloca 30 no R6 para fazer a comparação no branch
	add  $s6,$s6,$s1		#coloca 30 no R6 para fazer a comparação no branch
	
	#	MOVA A,0		#zera de novo o acumulador
	addi $s1,$zero,0		#zera de novo o acumulador
	
	#	MOV R3,A		#zera R3
	addi $s3,$zero,0		#zera o R3
	
	#	MOV R4,A		#zera R4
	addi $s4,$zero,0
	
	
loop:	#	MOVA A,0			#começamos o loop sempre zerando o acumulador
	addi $s1,$zero,0			#zera o acumulador
	
	#	ADD A,R3			#coloca R3 no acumulador
	add $s1,$s1,$s3
	
	#	ADD A,R4		#soma R4 com R3 no acumulador
	add $s1,$s1,$s4
	
	addi $s4,$zero,0
	
	#	MOV R4,A		#coloca o resultado em R4
	add $s4,$s4,$s1
	
	#	MOVA A,0		#zera novamente o acumulador
	addi $s1,$zero,0			#zera o acumulador
	
	#	MOV A,R3		#coloca R# no acumulador
	add $s1,$s1,$s3
	
	#	ADD A,1			#soma 1
	addi $s1,$s1,1
	
	addi $s3,$zero,0
	
	#	MOV R3,A		#coloca o resultado da soma em R3
	add $s3,$s3,$s1
	
	#	BNE A,R6,loop		#se a soma ainda não chegou em 30, volta pro loop
	
	bne $s1,$s6,loop
	
	#	MOV R4,R5		#coloca o resultado final em R5
	add $s4,$s4,$s5
